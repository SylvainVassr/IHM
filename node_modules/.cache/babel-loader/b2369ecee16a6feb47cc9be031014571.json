e = true;\r\n      nextTick(() => context.emit(\"ready\", leafletRef.value));\r\n    });\r\n\r\n    onBeforeUnmount(\r\n        () =>\r\n            leafletRef.value &&\r\n            leafletRef.value._leaflet_id &&\r\n            removeLayerFromMainMap({ leafletObject: leafletRef.value })\r\n    );\r\n\r\n    return { ready, leafletObject: leafletRef };\r\n  },\r\n  render() {\r\n    return render(this.ready, this.$slots);\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}
; margin: auto\">\r\n    <l-map\r\n        v-model=\"zoom\"\r\n        v-model:zoom=\"zoom\"\r\n        :center=\"[47, 3]\"\r\n        @move=\"log('move')\"\r\n    >\r\n      <l-tile-layer\r\n          url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\r\n      ></l-tile-layer>\r\n      <l-control-layers />\r\n      <l-marker :lat-lng=\"[0, 0]\" draggable @moveend=\"log('moveend')\">\r\n        <l-tooltip>\r\n          lol\r\n        </l-tooltip>\r\n      </l-marker>\r\n\r\n      <l-marker :lat-lng=\"[47.41322, -1.219482]\">\r\n        <l-icon :icon-url=\"iconUrl\" :icon-size=\"iconSize\" />\r\n      </l-marker>\r\n\r\n      <l-marker :lat-lng=\"[50, 50]\" draggable @moveend=\"log('moveend')\">\r\n        <l-popup>\r\n          lol\r\n        </l-popup>\r\n      </l-marker>\r\n    </l-map>\r\n    <button @click=\"changeIcon\">New kitten icon</button>\r\n  </div>\r\n</template>\r\n<script>\r\nimport {\r\n  LMap,\r\n  LIcon,\r\n  LTileLayer,\r\n  LMarker,\r\n  LControlLayers,\r\n  LTooltip,\r\n  LPopup,\r\n\r\n} from \"@vue-leaflet/vue-leaflet\";\r\nimport \"leaflet/dist/leaflet.css\";\r\nexport default {\r\n  props: {\r\n    apiArray : Array\r\n  },\r\n  components: {\r\n    LMap,\r\n    LIcon,\r\n    LTileLayer,\r\n    LMarker,\r\n    LControlLayers,\r\n    LTooltip,\r\n    LPopup,\r\n\r\n  },\r\n  data() {\r\n    return {\r\n      zoom: 6,\r\n      iconWidth: 25,\r\n      iconHeight: 40,\r\n    };\r\n  },\r\n  computed: {\r\n    iconUrl() {\r\n      return `https://cdn.icon-icons.com/icons2/37/PNG/512/raiway_station_4329.png`;\r\n    },\r\n    iconSize() {\r\n      return [this.iconWidth, this.iconHeight];\r\n    },\r\n  },\r\n  methods: {\r\n    changeIcon() {\r\n      this.iconWidth += 2;\r\n      if (this.iconWidth > this.iconHeight) {\r\n        this.iconWidth = Math.floor(this.iconHeight / 2);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"sourceRoot":""}]}